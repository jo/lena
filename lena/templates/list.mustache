<header>
  <h1 class=title><a href="/">{{title}}</a></h1>
  <nav id=menu>{{> menu}}</nav>
</header>
<section>
  <aside>&nbsp;</aside>
  <article>
    <ol class=list>
      {{#docs}}
        {{#doc}}
          <li data-id="{{_id}}"><a href="{{url}}">{{title}}</a> {{subtitle}}</li>
        {{/doc}}
        {{#docs}}
          <li class=double data-id="{{_id}}"><a href="{{url}}">{{title}}</a> {{subtitle}}</li>
        {{/docs}}
      {{/docs}}
    </ol>
  </article>
</section>
